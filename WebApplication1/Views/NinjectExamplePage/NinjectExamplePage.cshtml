@model IEnumerable<WebApplication1.Models.Users>

@{
    ViewBag.Title = "NinjectExamplePage";
}

@*<h2>NinjectExamplePage</h2>*@

<!DOCTYPE HTML>
<html>
    <head>
        <meta charset="utf-8">
        <title>NinjectExamplePage</title>
    </head>
    <body>
        <table style="border: 1px; border-color: black;">
            <caption>Таблица пользователей</caption>
            <tr>
                <th>Имя пользователя</th>
                <th>Псевдоним</th>
                <th>Email</th>
                <th>Номер телефона</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nickname</td>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td>@item.MobileNumber</td>
                </tr>
            }
        </table>
    </body>
</html>

